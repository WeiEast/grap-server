<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--运营商监控特殊处理,任务终态后发送相关消息到monitor-->
    <bean id="operatorMonitorSpecialProcessor"
          class="com.treefinance.saas.grapserver.biz.processor.OperatorMonitorSpecialProcessor">
        <property name="components">
            <set>
                <ref bean="taskCreateMonitorComponent"/>
                <ref bean="confirmMobileMonitorComponent"/>
                <ref bean="startLoginMonitorComponent"/>
                <ref bean="loginSuccessMonitorComponent"/>
                <ref bean="crawlSuccessMonitorComponent"/>
                <ref bean="processSuccessMonitorComponent"/>
                <ref bean="callbackSuccessMonitorComponent"/>
            </set>
        </property>
    </bean>


</beans>
